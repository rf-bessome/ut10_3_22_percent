	ОтсортированныеКодыМаркировки.СортироватьПоЗначению();
	
	URLЗапроса = СтрШаблон(
		"api/v3/true-api/cises/aggregated/list%1",
		ИнтерфейсИСМП.ПараметрыЗапроса(ПараметрыЗапроса));
		
