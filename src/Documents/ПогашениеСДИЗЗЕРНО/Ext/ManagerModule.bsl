Функция ИмяПоляНомерЗаявки(ВидПродукции)
	
	Если ВидПродукции = Перечисления.ВидыПродукцииИС.Зерно Тогда
		Возврат "extinctionId";
	Иначе
		Возврат "gpbExtinctionId";
	КонецЕсли;
	
КонецФункции

Процедура ДобавитьСообщениеДляРасчетаЗависимостей(ДанныеДляРасчетаЗависимостей, СообщениеXML)
	
	Если ДанныеДляРасчетаЗависимостей = Неопределено Тогда
		ДанныеДляРасчетаЗависимостей = Новый Соответствие;
	КонецЕсли;
	
	СДИЗ         = СообщениеXML.ПараметрыЗапроса.СДИЗ;
	ДанныеПоСДИЗ = ДанныеДляРасчетаЗависимостей[СДИЗ];
	Если ДанныеПоСДИЗ = Неопределено Тогда
		ДанныеПоСДИЗ = Новый Структура("ЕстьЧастичноеПогашение, ЕстьПолноеПогашение, Сообщения", Ложь, Ложь, Новый Массив);
		ДанныеДляРасчетаЗависимостей[СДИЗ] = ДанныеПоСДИЗ;
	КонецЕсли;
	
	Если СообщениеXML.ПараметрыЗапроса.ПолноеПогашение Тогда
		ДанныеПоСДИЗ.ЕстьПолноеПогашение = Истина;
	Иначе
		ДанныеПоСДИЗ.ЕстьЧастичноеПогашение = Истина;
	КонецЕсли;
	
	ДанныеПоСДИЗ.Сообщения.Добавить(СообщениеXML);
	
КонецПроцедуры

Процедура РассчитатьИЗаполнитьЗависмостиСообщений(ДанныеДляРасчетаЗависимостей)
	
	Для Каждого КлючИЗначение Из ДанныеДляРасчетаЗависимостей Цикл
		
		ДанныеПоСДИЗ = КлючИЗначение.Значение;
		
		Если ДанныеПоСДИЗ.Сообщения.Количество() = 1
			Или Не (ДанныеПоСДИЗ.ЕстьПолноеПогашение И ДанныеПоСДИЗ.ЕстьЧастичноеПогашение) Тогда
			Продолжить;
		КонецЕсли;
		
		ПередаватьПосле = Неопределено;
		ПоследнийИндекс = ДанныеПоСДИЗ.Сообщения.ВГраница();
		
		Для ТекушийИндекс = 0 По ПоследнийИндекс Цикл
			
			СообщениеXML = ДанныеПоСДИЗ.Сообщения[ПоследнийИндекс - ТекушийИндекс];
			СообщениеXML.ПередаватьПосле = ПередаватьПосле;
			
			ПередаватьПосле = СообщениеXML.Идентификатор;
			
		КонецЦикла;
		
	КонецЦикла;
	
КонецПроцедуры

